<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Webkul_MultiEbayStoreMageConnect::manager" title="Multi Store eBay Magento Connect" sortOrder="10" >
                    <resource id="Webkul_MultiEbayStoreMageConnect::ebay_account_connect" title="Connect eBay Account" sortOrder="10" >
                        <resource id="Webkul_MultiEbayStoreMageConnect::EbayAccount" title="Connect eBay Account" sortOrder="30"/>
                        <resource id="Webkul_MultiEbayStoreMageConnect::Categories" title="Map Categories" sortOrder="10"/>
                        <resource id="Webkul_MultiEbayStoreMageConnect::Products" title="Map Products" sortOrder="20"/>
                        <resource id="Webkul_MultiEbayStoreMageConnect::Ebayorder" title="Map Orders" sortOrder="30"/>
                    </resource>
                    <resource id="Webkul_MultiEbayStoreMageConnect::listing_templates" title="Listing Templates" sortOrder="10"/>
                    <resource id="Webkul_MultiEbayStoreMageConnect::products_pricerules" title="Price Rule" sortOrder="10"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Webkul_MultiEbayStoreMageConnect::config_multiebaystoremageconnect" title="Multi Store eBay Connect" sortOrder="50" >
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>   
        </resources>
    </acl>
</config>