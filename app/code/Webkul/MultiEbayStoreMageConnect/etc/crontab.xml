<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <!-- instance : contain class name which function we want to run on defined time -->
        <!-- method : contain method name which run on defined time -->
        <job name="multiebay_syn_order_cron" instance="Webkul\MultiEbayStoreMageConnect\Model\Cron" method="OrderSyncFromEbay">
            <schedule>*/20 * * * *</schedule>
        </job>
    </group>
</config>
